{
  "extends": "../../tsconfig/tsconfig.code-styles",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "allowJs": false
  },
  "include": ["src"]
}
