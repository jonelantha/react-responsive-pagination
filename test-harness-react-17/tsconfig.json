{
  "extends": "../tsconfig",
  "compilerOptions": {
    "paths": {
      "react": ["./node_modules/@types/react"]
    },
    "target": "ES2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "allowJs": false,
    "skipLibCheck": true
  },
  "include": ["src"]
}
